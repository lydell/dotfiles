# Dump all changed settings to stdout: `dconf dump /`
# You can also see many available settings using: `gsettings list-reqursively`
# dconf-editor is also useful.
# To apply this file: `dconf load / <$THIS_FILE`
# You might need to log out and log in for changes to take effect.

[org/gnome/settings-daemon/peripherals/touchscreen]
orientation-lock=true

[org/gnome/shell/calendar]
show-weekdate=true

[org/gnome/nautilus/preferences]
sort-directories-first=true

[org/gnome/settings-daemon/plugins/power]
ambient-enabled=false
sleep-inactive-battery-type='nothing'
sleep-inactive-ac-timeout=3600
sleep-inactive-ac-type='nothing'
sleep-inactive-battery-timeout=1800

[org/gnome/desktop/interface]
monospace-font-name='DejaVu Sans Mono 11'

[org/gnome/desktop/screensaver]
lock-enabled=true

[org/gnome/desktop/session]
idle-delay=uint32 900

[org/gnome/settings-daemon/plugins/orientation]
active=false

[org/gnome/desktop/peripherals/touchpad]
natural-scroll=true
tap-to-click=true

[org/gnome/desktop/input-sources]
sources=[('xkb', 'us+altgr-intl')]
xkb-options=['compose:sclk', 'terminate:ctrl_alt_bksp', 'lv3:ralt_switch']

[org/gnome/settings-daemon/peripherals/keyboard]
remember-numlock-state=false
numlock-state='on'

[org/gnome/mutter]
overlay-key=''

[org/gnome/shell/keybindings]
toggle-overview=['<Super>s', 'F13']

[org/gnome/desktop/wm/keybindings]
panel-main-menu=['<Alt>F1', 'F14']
close=['<Alt>F4', '<Super>q']
cycle-windows=['<Alt>Escape', '<Super>space']
cycle-windows-backward=['<Shift><Alt>Escape', '<Super><Shift>space']
show-desktop=['<Shift><Super>d']
switch-input-source=['']
switch-input-source-backward=['']

[org/gnome/settings-daemon/plugins/media-keys]
custom-keybindings=['/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom1/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom2/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom3/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom4/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom5/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom6/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom7/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom8/']

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0]
binding='<Shift><Super>q'
command='gnome-session-quit --power-off'
name='shut down'

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom1]
binding='<Super>h'
command='focus Gnome-terminal'
name='focus terminal'

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom2]
binding='<Super>t'
command="focus '(Atom|Gvim)'"
name='focus gvim'

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom3]
binding='<Super>r'
command="focus 'Firefox(?!.+Nightly$)'"
name='focus firefox'

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom4]
binding='<Super>period'
command="focus '(Chromium|Firefox.+Nightly$)'"
name='focus alternate browsers'

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom5]
binding='<Super>f'
command='focus Eog'
name='focus eog'

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom6]
binding='<Super>d'
command='focus Gimp'
name='focus gimp'

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom7]
binding='<Super>minus'
command='focus Slack'
name='focus slack'

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom8]
binding='<Super>w'
command='focus-or-start keepassxc keepassxc'
name='focus or start KeePassXC'

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom9]
binding='<Super>comma'
command="focus '(?!Gnome-terminal|Atom|Gvim|Firefox|Chromium|Eog|Gimp|Slack|keepassxc)'"
name='focus other'
