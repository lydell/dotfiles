{
    "breadcrumbs.enabled": true,
    "diffEditor.renderSideBySide": true,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.renderFinalNewline": false,
    "editor.renderWhitespace": "all",
    "files.insertFinalNewline": true,
    "html.autoClosingTags": false,
    "html.mirrorCursorOnMatchingTag": false,
    "javascript.suggest.autoImports": false,
    "javascript.validate.enable": false,
    "keyboard.dispatch": "keyCode",
    "vim.camelCaseMotion.enable": true,
    "vim.changeWordIncludesWhitespace": true,
    "vim.easymotionKeys": "ehstirnoamupcwlfgdy",
    "vim.easymotionMarkerFontFamily": "",
    "vim.easymotionMarkerHeight": 18,
    "vim.easymotion": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.leader": "Space",
    "vim.sneak": true,
    "vim.useCtrlKeys": false,
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "Y"
            ],
            "after": [
                "y",
                "$"
            ]
        },
        {
            "before": [
                "Delete"
            ],
            "after": [
                "x"
            ]
        },
        {
            "before": [
                "Backspace"
            ],
            "after": [
                "X"
            ]
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "p",
            ],
            "after": [
                "p",
                "g",
                "v",
                "y",
                "g",
                "v",
                "Escape"
            ]
        },
        {
            "before": [
                "y",
            ],
            "after": [
                "y",
                "g",
                "v",
                "Escape"
            ]
        },
        {
            "before": [
                "g",
                "c",
            ],
            "after": [
                "g",
                "c",
                "g",
                "v",
                "Escape"
            ]
        },
    ],
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.requireConfig": true,
    "window.zoomLevel": 1,
    "editor.fontSize": 13,
    "editor.codeActionsOnSave": {
        "source.fixAll.tslint": true,
        "source.fixAll.eslint": true
    },
    "editor.renameOnType": false,
    "editor.minimap.enabled": false,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "git.detectSubmodulesLimit": 20,
    "git.confirmSync": false,
    "git.suggestSmartCommit": false,
    "scm.alwaysShowProviders": true,
    "extensions.ignoreRecommendations": false,
    "FSharp.showExplorerOnStartup": false,
    "FSharp.useSdkScripts": true,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gremlins.characters": {
        "00a0": {
            "description": "non breaking space",
            "level": "warning"
        },
        "201c": {
            "description": "left double quotation mark",
            "level": "info"
        },
        "201d": {
            "description": "right double quotation mark",
            "level": "info"
        }
    },
    "gremlins.color_info": "transparent",
    "gremlins.gutterIconSize": "0%",
    "workbench.colorCustomizations": {
        "list.focusBackground": "#7f7f7f7f",
    }
}
